{
    "name": "Z-Player",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A extension for playing music",
    "action":{
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        },
        "default_popup": "popup.html"
    },
    "permissions":[
        "storage"
    ]
}